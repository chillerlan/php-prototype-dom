{
	"name": "chillerlan/php-prototype-dom",
	"description": "Prototype.js like DOM traversal and manipulation. PHP 7.4+",
	"homepage": "https://github.com/chillerlan/php-prototype-dom",
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-prototype-dom/issues",
		"source": "https://github.com/chillerlan/php-prototype-dom"
	},
	"minimum-stability": "stable",
	"require": {
		"php": "^7.4",
		"ext-dom": "*",
		"ext-libxml": "*",
		"symfony/css-selector": "^4.2"
	},
	"require-dev": {
		"phan/phan": "^2.7",
		"phpunit/phpunit": "^9.1"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\PrototypeDOM\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\PrototypeDOMTest\\": "tests",
			"chillerlan\\PrototypeDOMExamples\\": "examples"
		}
	}
}
